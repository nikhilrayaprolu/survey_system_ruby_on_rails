<div class="container"><h1>Listing active Questions</h1>

<table>
  <thead>
    <tr>
      <th>Text</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @active.each do |question| %>
      <tr>
        <td><%= question["question"] %></td>
        <td><%= link_to "show", controller: "questions", action: "show", id: question["id"] %></td>
        

       </tr>
    <% end %>
  </tbody>
</table>

<br>

</div>